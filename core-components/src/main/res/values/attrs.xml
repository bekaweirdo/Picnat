<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextWithTitle">
        <attr name="EditTextWithTitle_Hint" format="string"/>
        <attr name="EditTextWithTitle_HintColor" format="color"/>
        <attr name="EditTextWithTitle_Text" format="string"/>
        <attr name="EditTextWithTitle_TextColor" format="color"/>
        <attr name="EditTextWithTitle_InputType" format="integer">
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
            <enum name="decimal" value="3"/>
            <enum name="email" value="4"/>
            <enum name="password" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedButton">
        <attr name="RoundedButton_Color" format="color"/>
        <attr name="RoundedButton_StrokeColor" format="color"/>
        <attr name="RoundedButton_Text" format="string"/>
        <attr name="RoundedButton_TextSize" format="integer"/>
        <attr name="RoundedButton_TextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SPSlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension"/>
        <attr name="umanoShadowHeight" format="dimension"/>
        <attr name="umanoParallaxOffset" format="dimension"/>
        <attr name="umanoFadeColor" format="color"/>
        <attr name="umanoFlingVelocity" format="integer"/>
        <attr name="umanoDragView" format="reference"/>
        <attr name="umanoScrollableView" format="reference"/>
        <attr name="umanoOverlay" format="boolean"/>
        <attr name="umanoClipPanel" format="boolean"/>
        <attr name="umanoAnchorPoint" format="float"/>
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0"/>
            <enum name="collapsed" value="1"/>
            <enum name="anchored" value="2"/>
            <enum name="hidden" value="3"/>
        </attr>
        <attr name="umanoScrollInterpolator" format="reference"/>
    </declare-styleable>
</resources>